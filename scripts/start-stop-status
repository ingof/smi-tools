#!/bin/sh

case $1 in
	start)
		echo "UsbSerial1m" > $SYNOPKG_TEMP_LOGFILE 
		insmod /lib/modules/usbserial.ko > $SYNOPKG_TEMP_LOGFILE 
		echo "UsbSerial2m" > $SYNOPKG_TEMP_LOGFILE 
		insmod /lib/modules/usbserial.ko > $SYNOPKG_TEMP_LOGFILE 
		echo "UsbSerial3o" > $SYNOPKG_TEMP_LOGFILE 
		insmod /lib/modules/usbserial.ko
		echo "UsbSerial4o" > $SYNOPKG_TEMP_LOGFILE 
		insmod /lib/modules/usbserial.ko
		echo "ftdi_sio" > $SYNOPKG_TEMP_LOGFILE 
		insmod /lib/modules/ftdi_sio.ko
		exit 0
		;;
	stop)
		exit 0
		;;
	status)
		exit 0
		;;
	log)
		exit 0
		;;
	*)
		exit 1
		;;
esac
